                    <div class="col-md-9">
                        <p class="form-control-static"> @Html.DisplayFor(m => m.CreateDate) </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                @Html.LabelFor(a => a.FullMessage, new { @class = "control-label col-md-3" })
                <div class="col-md-10">
                    <p class="form-control-static"> @Html.DisplayFor(m => m.FullMessage) </p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                @Html.LabelFor(a => a.CreateDate, new { @class = "control-label col-md-2" })
