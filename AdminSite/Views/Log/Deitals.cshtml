                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    @Html.LabelFor(a => a.PageUrl, new { @class = "control-label col-md-3" })
                    <div class="col-md-9">
                        <p class="form-control-static"> @Html.DisplayFor(m => m.PageUrl) </p>
                    </div>
                </div>
            </div>
