@using ViewModels.AdminWeb.Log
@model ViewModels.AdminWeb.Log.LogViewModel
<div class="form">
    <div class="form-body">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    @Html.LabelFor(a => a.KeyWord, new {@class = "control-label col-md-3"})
                    <div class="col-md-9">
                        <p class="form-control-static"> @Html.DisplayFor(m => m.KeyWord) </p>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    @Html.LabelFor(a => a.ShortMessage, new {@class = "control-label col-md-3"})
                    <div class="col-md-9">
                        <p class="form-control-static"> @Html.DisplayFor(m => m.ShortMessage) </p>
